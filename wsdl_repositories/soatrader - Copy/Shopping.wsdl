					</xs:documentation>
					                <xs:appinfo>
						                  <CallInfo>
							                    <CallName>GetShippingCosts</CallName>
							                    <Returned>Conditionally</Returned>
						                  </CallInfo>
					                </xs:appinfo>
				              </xs:annotation>
			            </xs:element>
			            <xs:element name="ShippingServiceAdditionalCost" type="ns:AmountType" minOccurs="0">
