						                     </CallInfo>
						                     <CallInfo>
							                       <CallName>GetSingleItem</CallName>
							                       <Returned>Conditionally</Returned>
						                     </CallInfo>
						                     <CallInfo>
							                       <CallName>FindItems</CallName>
							                       <Returned>Conditionally</Returned>
						                     </CallInfo>
					                   </xs:appinfo>
				                 </xs:annotation>
			               </xs:attribute>
		             </xs:extension>
	           </xs:simpleContent>
         </xs:complexType>
