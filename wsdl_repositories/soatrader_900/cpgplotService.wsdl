                  xmlns:intf="http://www.dkfz-heidelberg.de/menu/tomcat/as/nucleic_cpg_islands.cpgplot"
                  xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  targetNamespace="http://www.dkfz-heidelberg.de/menu/tomcat/as/nucleic_cpg_islands.cpgplot">
