            <restriction base="xsd:string">
               <enumeration value="O"/>
               <enumeration value="A"/>
               <enumeration value="X"/>
               <enumeration value="N"/>
            </restriction>
         </simpleType>
         <complexType name="Tlistor">
            <sequence>
               <element name="firstset" type="impl:TlistorFirstset">
                  <xsd:annotation>
                     <xsd:documentation>qualifiers for parameter: firstset</xsd:documentation>
                  </xsd:annotation>
               </element>
               <element name="secondset" type="impl:TlistorSecondset">
                  <xsd:annotation>
                     <xsd:documentation>qualifiers for parameter: secondset</xsd:documentation>
                  </xsd:annotation>
               </element>
               <element maxOccurs="1" minOccurs="0" name="operator" type="impl:TlistorOperator">
                  <xsd:annotation>
                     <xsd:appinfo xmlns:emboss="http://emboss.sourceforge.net/schema/prompt">
                        <emboss:prompt>Logical operator to combine sequence lists</emboss:prompt>
                     </xsd:appinfo>
                     <xsd:documentation>The following logical operators combine the sequences in
           the following ways: 
           OR - gives all that occur in one set or the other 
           AND - gives only those which occur in both sets 
           XOR - gives those which only occur in one set or the other, but
           not in both 
           NOT - gives those which occur in the first set except for those
           that also occur in the second</xsd:documentation>
                  </xsd:annotation>
               </element>
            </sequence>
         </complexType>
